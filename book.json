{
  "title": "Golang Web",
  "description": "一本Go语言Web开发教程书籍!",
  "language": "zh-hans",
  "gitbook": "2.x.x",
  "styles": {
    "website": "style.css"
  },
  "variables": {
    "dot": "{{.}}",
    "empty": "{{}}",
    "define": "{{define}}",
    "_with": "{{with …}}",
    "with": "{{with}}",
    "range": "{{range}}",
    "_range": "{{range …}}",
    "end": "{{end}}",
    "else": "{{else}}",
    "blocks": "{{}}",
    "_createTrans": "{{.create | Trans}}",
    "_timeTrans": "{{.time | TransDate}}",
    "_moneyTrans": "{{.money | TransMoney}}",
    "_formRender": "{{.form.render()}}",
    "_errorInfo": "{{.ErrorInfo}}",
    "_vvlang": "{{.VV.Lang}}",
    "_vSubmit": "{{.V.Submit | T}}",
    "_vNowTD": "{{.V.Now | TD}}",
    "_vMoney": "{{.V.Money | M}}",
    "_if": "{{if ``}}",
    "field": "{{.FieldName}}",
    "username": "{{.UserName}}",
    "email":"{{.email}}",
    "_html":"{{. | html}}",
    "_emailDeal":"{{.|emailDeal}}",
    "_rangeEmails": "{{range .Emails}}",
    "_withFriends":"{{with .Friends}}",
    "_rangeDot": "{{range .}}",
    "_Fname":"{{.Fname}}",
    "_Name":"{{.Name}}",
    "_Header": "{{define \"header\"}}",
    "_Content": "{{define \"content\"}}",
    "_Footer": "{{define \"footer\"}}",
    "_TemplateHeader": "{{template \"header\"}}",
    "_TemplateFooter": "{{template \"footer\"}}",
    "_childTemplate":"{{define \"子模板名称\"}}",
    "_rangeEmail":"{{range .Emails}}",
    "_output1":"{{with $x := \"output\" | printf \"%q\"}}\n\t{{$x}}\n{{end}}",
    "_output2":"{{with $x := \"output\"}}\n\t{{printf \"%q\" $x}}\n{{end}}",
    "_output3":"{{with $x := \"output\"}}\n\t{{$x | printf \"%q\"}}\n{{end}}",
    "_ifAnyThing": "{{if `anything`}}",
    "_layoutContent": "{{.LayoutContent}}",
    "_rangeBlogs": "{{range .blogs}}",
    "_title": "{{.Title}}",
    "_created": "{{.Created}}",
    "_postTitle": "{{.Post.Title}}",
    "_postId": "{{.Post.Id}}",
    "_postCreated": "{{.Post.Created}}",
    "_postContent": "{{.Post.Content}}"
  }
}
